#include<stdio.h>
#include<string.h>

int main()
{
    char A;
    char p1[20], p2[20], p3[20];

    printf("Enter grammar (Example: S->iEtS|iEtSeS|a)\n");
    scanf("%c->%[^|]|%[^|]|%s", &A, p1, p2, p3);

    printf("\nAfter left factoring:\n");

    printf("%c -> iEtS%c' | %s\n", A, A, p3);
    printf("%c' -> eS | @\n", A);

    printf("E -> b\n");

    return 0;
}
